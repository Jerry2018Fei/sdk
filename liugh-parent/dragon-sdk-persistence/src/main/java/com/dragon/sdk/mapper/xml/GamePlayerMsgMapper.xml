<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dragon.sdk.mapper.GamePlayerMsgMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.dragon.sdk.entity.GamePlayerMsg">
        <id column="id" property="id"/>
        <result column="imei" property="imei"/>
        <result column="ip" property="ip"/>
        <result column="device_id" property="deviceId"/>
        <result column="create_time" property="createTime"/>
        <result column="status" property="status"/>
        <result column="delete_flag" property="deleteFlag"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id , mobile, imei, ip,device_id as deviceId, unit,
create_time AS createTime,status, delete_flag as deleteFlag
    </sql>


</mapper>
